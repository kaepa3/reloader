{
  "name": "reloader",
  "manifest_version": 3,
  "version": "0.1",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "service.js"
  },
  "action": {
    "default_icon": "popup16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
